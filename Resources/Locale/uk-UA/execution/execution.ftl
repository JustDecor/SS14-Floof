execution-verb-name = Виконати
execution-verb-message = Використовуйте свою зброю, щоб когось стратити.

# All the below localisation strings have access to the following variables
# attacker (the person committing the execution)
# victim (the person being executed)
# weapon (the weapon used for the execution)

execution-popup-melee-initial-internal = Ви готуєте {THE($weapon)} проти горла {$victim}.
execution-popup-melee-initial-external = {$attacker} готує {$weapon} проти горла {$victim}.
execution-popup-melee-complete-internal = Ви ріжете горло {$victim}!
execution-popup-melee-complete-external = {$attacker} ріже горло {$victim}!

execution-popup-self-initial-internal = Ви готуєте {THE($weapon)} до свого горла.
execution-popup-self-initial-external = {$attacker} готує {POSS-ADJ($attacker)} {$weapon} до свого горла.
execution-popup-self-complete-internal = Ти перерізав собі горло!
execution-popup-self-complete-external = {$attacker} перерізує собі горло!
